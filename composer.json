		"nette/security": "^2.4",
		"nette/utils": "^2.4",
		"latte/latte": "^2.4",
		"tracy/tracy": "^2.4",
		"dg/adminer-custom": "^1.8",
		"phpstan/phpstan-nette": "^0.10.0@dev",
		"nextras/orm": "^3.0@dev"
